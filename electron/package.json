{
  "name": "blenderrenderqueue",
  "productName": "Blender Render Queue",
  "version": "1.0.0",
  "description": "Standalone render queue for Blender",
  "author": {
    "name": "St√©phane Claudon",
    "email": ""
  },
  "license": "MIT",
  "main": "build/src/index.js",
  "scripts": {
    "build": "tsc && electron-rebuild",
    "electron:start-live": "node ./live-runner.js",
    "electron:start": "npm run build && electron --inspect=5858 ./",
    "electron:pack": "npm run build && electron-builder build --mac -c ./electron-builder.config.json",
    "electron:make": "npm run build && electron-builder build --mac --win --linux -c ./electron-builder.config.json -p always"
  },
  "dependencies": {
    "@capacitor-community/electron": "^4.1.1",
    "chokidar": "~3.5.3",
    "command-exists-promise": "^2.0.2",
    "electron-is-dev": "~2.0.0",
    "electron-serve": "~1.1.0",
    "electron-store": "^8.1.0",
    "electron-unhandled": "~4.0.1",
    "electron-updater": "~5.0.1",
    "electron-window-state": "~5.0.3",
    "psuspend": "^1.1.0"
  },
  "devDependencies": {
    "electron": "^23.1.0",
    "electron-builder": "~23.6.0",
    "electron-packager": "^17.1.1",
    "electron-rebuild": "^3.2.7",
    "typescript": "~4.3.5"
  },
  "keywords": [
    "capacitor",
    "electron"
  ]
}